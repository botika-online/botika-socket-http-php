{
    "name": "botika/socket",
    "description": "Library for interacting with the Botika Socket REST API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Botika\\Socket\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andri Anto",
            "email": "andrianto.ykhc@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^7.0|^6.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
    },
    "minimum-stability": "dev",
	"prefer-stable": true
}
